<!DOCTYPE html>

<html>
<head>
  
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Google Fonts -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js"></script>
    <script>
        WebFont.load({
            google: {"families":["Montserrat:400,500,600,700","Noto+Sans:400,700"]},
            active: function() {
                sessionStorage.fonts = true;
            }
        });
    </script>
    <!-- Favicon -->
    <link rel="apple-touch-icon" sizes="180x180" href=@Url.Content("/Content/img/apple-touch-icon.png")>
    <link rel="icon" type="image/png" sizes="32x32" href=@Url.Content("/Content/img/favicon-32x32.png")>
    <link rel="icon" type="image/png" sizes="16x16" href=@Url.Content("/Content/img/favicon-16x16.png")>
    <!-- Stylesheet -->
    <link rel="stylesheet" href=@Url.Content("/Content/vendors/css/base/bootstrap.min.css")>
    <link rel="stylesheet" href=@Url.Content("/Content/vendors/css/base/elisyam-1.5.min.css")>
    <link rel="stylesheet" href=@Url.Content("/Content/css/animate/animate.min.css")>
    <!-- Tweaks for older IEs--><!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
</head>
 
 
<body class="bg-white">
<div>
    @RenderBody()
</div>

<!-- End Container -->
<!-- Begin Vendor Js -->
<script src=@Url.Content("/Content/vendors/js/base/jquery.min.js")></script>
<script src=@Url.Content("/Content/vendors/js/base/core.min.js")></script>
<!-- End Vendor Js -->
<!-- Begin Page Vendor Js -->
<script src=@Url.Content("/Content/vendors/js/app/app.min.js")></script>
<!-- End Page Vendor Js -->
<!-- Begin Page Snippets -->
<script src=@Url.Content("/Content/js/components/tabs/animated-tabs.min.js")></script>
<!-- End Page Snippets -->
</body>
</html>
